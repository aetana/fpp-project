package day2.prog7;

public class Prog7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String output = "";
		output = String.format("%16d %12d %12d %n %n", genNum(), genNum(), genNum());
		output += String.format("%13s %2d %9s %2d %9s %2d%n","+", genNum(), "+", genNum(), "+", genNum());
		output += String.format("%16s %12s %12s","____", "____","____");
		System.out.println(output);
		
		

	}
	
	public static int genNum() {
		return RandomNumbers.getRandomInt(1, 99);
	}

}
